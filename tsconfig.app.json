{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/app/**/*.ts",
    "src/main.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*.tsx",
    "src/components/**",
    "src/pages/**",
    "src/hooks/**",
    "src/App.tsx",
    "src/main.tsx",
    "src/vite-env.d.ts"
  ]
}
